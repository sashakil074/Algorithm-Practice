    "def acarman(m,n):\n",
    "    if m==0:\n",
    "        return n+1\n",
    "    elif m>0 and n==0:\n",
    "        return acarman(m-1,1)\n",
    "    elif m>0 and n>0:\n",
    "        return acarman(m-1,acarman(m,n-1))"
   ]
